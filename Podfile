source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!

target :'CartonBox' do
    
    # Pods for AWS
    pod 'AWSCognito'
    pod 'AWSCognitoIdentityProvider'
    pod 'AWSDynamoDB'
    pod 'AWSLambda'
    pod 'AWSRekognition'
    pod 'AWSS3'
    pod 'AWSSimpleDB'
    pod 'AWSSNS'
    pod 'AWSSQS'
    pod 'AWSAuthCore'
    pod 'AWSFacebookSignIn'
    pod 'AWSAuthUI'
    
    #Pods for FB SDK
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
    
    #Pods for KeyChain
    pod 'UICKeyChainStore'
    
    # Pods for CartonBox
    pod 'XLPagerTabStrip'
    pod 'Bond'
    pod 'Player', '~> 0.8.0'
    
  target 'CartonBoxTests' do
    inherit! :search_paths
    # Pods for testing
  end
end

